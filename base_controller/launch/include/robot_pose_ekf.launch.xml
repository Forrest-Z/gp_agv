<launch>
    <node pkg="base_controller" type="imu_get" name="imu_get"/>

	<node pkg="robot_pose_ekf" type="robot_pose_ekf" name="robot_pose_ekf">
		<param name="output_frame" value="odom"/>
		<param name="base_footprint_frame" value="base_footprint"/>
		<param name="freq" value="30.0"/><!--频率-->
		<param name="sensor_timeout" value="1.0"/>
		<param name="odom_used" value="true"/><!--是否使用里程计-->
		<param name="imu_used" value="true"/><!--是否使用IMU-->
		<param name="vo_used" value="false"/>
		<param name="debug" value="false"/>
		<param name="self_diagnose" value="false"/>
        <remap from="odom" to="odom_data" />
	</node>
    
    <node pkg="base_controller" type="pub_odom" name="pub_odom"/>
</launch>